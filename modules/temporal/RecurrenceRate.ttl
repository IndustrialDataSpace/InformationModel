@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

@prefix ids_temp: <https://schema.industrialdataspace.org/temporal/> .

# Classes
# -------

ids_temp:RecurrenceRate a owl:Class;
    rdfs:label "RecurrenceRate"@en ;
    rdfs:comment "The class of discrete recurrence rates.".

# Instances
# ---------

ids_temp:hourly a ids_temp:RecurrenceRate;
    rdfs:comment "Hourly";
    dct:identifier "HOURLY".

ids_temp:daily a ids_temp:RecurrenceRate;
    rdfs:comment "Daily";
    dct:identifier "DAILY".

ids_temp:weekly a ids_temp:RecurrenceRate;
    rdfs:comment "Weekly";
    dct:identifier "WEEKLY".

ids_temp:monthly a ids_temp:RecurrenceRate;
    rdfs:comment "Monthly";
    dct:identifier "MONTHLY".

ids_temp:yearly a ids_temp:RecurrenceRate;
    rdfs:comment "Yearly";
    dct:identifier "YEARLY".

