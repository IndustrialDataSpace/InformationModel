    sh:property shapes:RolebasedPermissionTemplate-constraintPropertyShape ;
    sh:property shapes:RolebasedPermissionTemplate-constraintPropertyForMemberShape ;
    sh:property shapes:RolebasedPermissionTemplate-constraintPropertyForMembershipShape ;
